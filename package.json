    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "moleculer-docgen": "^0.3.0",
    "moleculer-repl": "^0.6.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
